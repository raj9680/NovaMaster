<div id="aboutme" class="tab-pane active">
    <h5 class="mt-4">User Information</h5>
    <div class="row profile-desk">
        <table class="col-sm-6 table table-condensed mb-0">
            <tbody>
                <tr>
                    <th scope="row">Full Name</th>
                    <td>
                        <a href="#" class="ng-binding">
                            @ViewBag.ClientInfo[0]["FullName"]
                        </a>
                    </td>
                </tr>
                <tr>
                    <th scope="row">Email</th>
                    <td>
                        <a href="vertical.html" class="ng-binding">
                            @ViewBag.ClientInfo[0]["Email"]
                        </a>
                    </td>
                </tr>
                <tr>
                    <th scope="row">Contact Number</th>
                    <td class="ng-binding">
                        @ViewBag.ClientInfo[1]["ContactNumber"]
                    </td>
                </tr>
                <tr>
                    <th scope="row">DOB</th>
                    <td>
                        <a href="#" class="ng-binding">
                            @ViewBag.ClientInfo[1]["DOB"]
                        </a>
                    </td>
                </tr>
            </tbody>
        </table>
        <table class="col-sm-6 table table-condensed mb-0">
            <tbody>
                <tr>
                    <th scope="row">Address 1</th>
                    <td>
                        <a href="#" class="ng-binding">
                            @ViewBag.ClientInfo[1]["AddressLine1"]
                        </a>
                    </td>
                </tr>
                <tr>
                    <th scope="row">Address 2</th>
                    <td>
                        <a href="vertical.html" class="ng-binding">
                            @ViewBag.ClientInfo[1]["AddressLine2"]
                        </a>
                    </td>
                </tr>
                <tr>
                    <th scope="row">City</th>
                    <td class="ng-binding">
                        @ViewBag.ClientInfo[1]["CityId"]
                    </td>
                </tr>
                <tr>
                    <th scope="row">Zip</th>
                    <td>
                        @ViewBag.ClientInfo[1]["Zip"]
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
