    @*  *@
    <h5 class="mt-4">Document Details</h5>
    <div class="row profile-desk">
        <table class="col-sm-6 table table-condensed mb-0">
            <tbody>
                @if (ViewBag.ClientInfo[2].Count > 0)
                {
                for (int i = 0; i < ViewBag.ClientInfo[2].Count; i++)
                {
                if (ViewBag.ClientInfo[2][i]["Type"] == "passport")
                {
                <tr>
                    <th scope="row">Passport</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                if (ViewBag.ClientInfo[2][i]["Type"] == "exam")
                {
                <tr>
                    <th scope="row">Exam Type</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                if (ViewBag.ClientInfo[2][i]["Type"] == "matriculation")
                {
                <tr>
                    <th scope="row">Matriculation</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                if (ViewBag.ClientInfo[2][i]["Type"] == "secondary")
                {
                <tr>
                    <th scope="row">Secondary</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                if (ViewBag.ClientInfo[2][i]["Type"] == "bachelors")
                {
                <tr>
                    <th scope="row">Bachelors</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                if (ViewBag.ClientInfo[2][i]["Type"] == "experience")
                {
                <tr>
                    <th scope="row">Job Experience Letter</th>
                    <td>
                        <a href="@ViewBag.ClientInfo[2][i][" DocumentURL"]" class="ng-binding">
                            @ViewBag.ClientInfo[2][i]["DocumentName"]
                        </a>
                    </td>
                </tr>
                }
                }
                }
            </tbody>
        </table>
    </div>
</div>